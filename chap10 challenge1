public class Shape {
	private String type;
	private String color;
	private Double area;
	
	public Shape(String type, String color, Double area) {
		this.type = type;
		this.color = color;
		this.area = area;
	}
	
	public String getColor() {
		return color;
	}
	
	public String getType() {
		return color;
	}
	
	public Double getArea() {
		return area;
	}
	
	public String toString() {
		return "도형 타입 : " + type + "(" + color + ", " + area + ")";
	}
}
